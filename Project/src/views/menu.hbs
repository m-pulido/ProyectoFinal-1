  {{#if user}}
    <div class="container pt-5 pb-5">
      <div class="row ">
        <div class="card-deck  text-center" >
          <div class="card" >
            <img class="card-img-top h-25 w-25" src="img/impexp.svg" alt="Card image cap">
            <div class="card-body">
              <a href="#" style="color:#1e636d"><h5 class="card-title" >Impotar/Exportar</h5></a>
              <p class="card-text">Importar o Exportar datos a la base de datos.</p>
            </div>
          </div>
          <div class="card" >
            <img class="card-img-top h-25 w-25" src="img/agregar.svg" alt="Card image cap">
            <div class="card-body">
              <a href="#" style="color:#1e636d"><h5 class="card-title">Agregar Registros</h5></a>
              <p class="card-text">Agregar beneficiarios a la base de datos.</p>
            </div>
          </div>
          <div class="card" >
            <img class="card-img-top h-25 w-25" src="img/lista.svg" alt="Card image cap">
            <div class="card-body">
              <a href="#" style="color:#1e636d"><h5 class="card-title">Consulta/Notificación de beneficiarios</h5></a>
              <p class="card-text">Enlista los datos de los beneficiarios y si han sido contactados o no.</p>
            </div>
          </div>
        </div>
      </div>
      <div class="row mt-3 ">
        <div class="card-deck  text-center">
          <div class="card" >
            <img class="card-img-top h-25 w-25" src="img/asistencia.svg" alt="Card image cap">
            <div class="card-body">
              <a href="#" style="color:#1e636d"><h5 class="card-title">Registro de asistencia</h5></a>
              <p class="card-text">Registro de asistencia de beneficiarios.</p>
            </div>
          </div>
          <div class="card">
            <img class="card-img-top h-25 w-25" src="img/entrega.svg" alt="Card image cap">
            <div class="card-body">
              <a href="#" style="color:#1e636d"><h5 class="card-title">Registro de entrega</h5></a>
              <p class="card-text">Registro de los beneficiarios que recibieron los apoyos el día del evento.</p>
            </div>
          </div>
